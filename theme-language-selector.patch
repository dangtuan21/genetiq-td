+:root {
+	/* Light Theme (default) */
+	--bg-primary: #ffffff;
+	--bg-secondary: #f3f4f6;
+	--bg-hover: #f1f5f9;
+	--text-primary: #1f2937;
+	--text-secondary: #4b5563;
+	--border-color: #e5e7eb;
+	--primary: #4f46e5;
+	--primary-hover: #4338ca;
+	--success: #10b981;
+	--error: #ef4444;
+	--warning: #f59e0b;
+	--info: #3b82f6;
+}
+
+/* Dark Theme */
+.dark {
+	--bg-primary: #111827;
+	--bg-secondary: #1f2937;
+	--bg-hover: #1e40af;
+	--text-primary: #f9fafb;
+	--text-secondary: #e5e7eb;
+	--border-color: #374151;
+	--primary: #818cf8;
+	--primary-hover: #6366f1;
+	--success: #34d399;
+	--error: #f87171;
+	--warning: #fbbf24;
+	--info: #60a5fa;
+}
+
+/* Base styles */
+body {
+	background-color: var(--bg-primary);
+	color: var(--text-primary);
+	transition:
+		background-color 0.3s ease,
+		color 0.3s ease;
+}
